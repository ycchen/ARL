<div class="root">
	<div class="row">
		<div class="span12">
			<div class="grey-box">
				<h3>Users</h3>
				<table class="table table-striped">
	              <tr>
	                <th>Name</th>
	                <th>Email</th>
	              </tr>

	            <% @users.each do |user| %>
	              <tr>
	              	<td><%= user.display_name %></td>
	              	<td><%= user.email %></td>
	              </tr>
	            <% end %>
	            </table>

	            <br />
			</div>
		</div>
	</div>
</div>
